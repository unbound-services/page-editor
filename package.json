{
  "name": "unb-editor",
  "version": "0.3.3",
  "private": false,
  "description": "Drop-In document editor to generate webpages and pdfs",
  "author": {
    "name": "Unbound"
  },

  "exports": {

    "./without-react": {
      "types": "./pkg_build/types/index.d.ts",
      "require": "./pkg_build/with-react/index.cjs.js",
      "import": "./pkg_build/with-react/index.es.js"
    },
    "./with-react": {
      "types": "./pkg_build/types/index.d.ts",
      "require": "./pkg_build/with-react/index.cjs.js",
      "import": "./pkg_build/with-react/index.es.js"
    },
    "./unb-editor.css": {
      "import": "./pkg_build/with-react/unb-editor.css",
      "require": "./pkg_build/with-react/unb-editor.css"
    },
    ".": {
      "types": "./pkg_build/types/index.d.ts",
      "require": "./pkg_build/with-react/index.cjs.js",
      "import": "./pkg_build/with-react/index.es.js"

    }
  },
  "main": "pkg_build/without-react/index.cjs.js",
  "module": "pkg_build/without-react/index.es.js",
  "types": "pkg_build/types/index.d.ts",
  "files": [
    "pkg_build/",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/unbound-services/page-editor "
  },
    
  "dependencies": {

  },
  "scripts": {
    "watch": "vite",
    "watch-webpack": "webpack watch --config webpack.config.js",
    "build": "vite build --config vite.build.config.js --mode production --emptyOutDir && vite build --config vite.build.config.js --mode with-react --emptyOutDir",
    "preview": "vite preview",
    "build-webpack": "webpack --mode production --config webpack.config.js",
    "package": "rimraf /F /Q pkg_build && babel src/lib --out-dir pkg_build --copy-files && yarn pack",
    "bundle": "rimraf /F /Q pkg_build && webpack --mode production --config webpack.config.js --entry /src/index.ts -o pkg_build",
    "dev-bundle": "rimraf /F /Q pkg_build && webpack --mode development --config webpack-devbundle.config.js --entry /src/index.ts -o pkg_build",
    "test-api": "ts-node testing-api-server/test-api.ts",
    "test": "node --experimental-vm-modules  node_modules/jest/bin/jest.js --config jest.config.js",
    "test-bundle": "webpack --config ./webpack.config.test.js",
    "deprecated-types": "tsc --project types.tsconfig.json --emitDeclarationOnly",
    "build-server-side": "webpack --mode production --config server-side-webpack.config.js --entry ./src/server-side.tsx -o pkg_build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@preact/preset-vite": "^2.10.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/user-event": "^14.5.2",
    "@types/diff": "^7.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "buffer": "^6.0.3",
    "canvas": "^3.0.1",
    "css-loader": "^7.1.2",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "express": "^4.21.2",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "^0.1.2",
    "ignore-styles": "^5.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-preact": "^4.1.1",
    "jsdom": "^26.0.0",
    "mini-css-extract-plugin": "^2.9.2",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "preact": "^10.25.3",
    "preact-render-to-string": "^6.5.12",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "sass": "^1.83.1",
    "sass-loader": "^16.0.4",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.11",
    "vite-bundle-analyzer": "^0.16.1",
    "vite-plugin-dts": "^4.5.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0",
    "ws": "^8.18.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "diff": "^7.0.0",
    "dts-bundle-generator": "^9.5.1",
    "ipsumlorem": "^1.0.2",
    "jsdom-global": "^3.0.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "types-webpack-bundler": "^1.0.2",
    "web-vitals": "^4.2.4"
  },
  "overrides": {
    "jest-environment-jsdom": {
      "canvas": "^3.0.1"
    }
  }
}
